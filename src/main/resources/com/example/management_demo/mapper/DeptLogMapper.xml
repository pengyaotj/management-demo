        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.management_demo.mapper.DeptLogMapper">

    <insert id="insert">
        insert
        into dept_log(create_time,desc)
        values(#{createTime},#{description})
    </insert>
</mapper>